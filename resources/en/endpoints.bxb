endpoints {
  action-endpoints {
    action-endpoint (RegisterDrink) {
      accepted-inputs (drinkType, drinkPercentage)
      local-endpoint ("RegisterDrink.js")
    }
    action-endpoint (RegisterBodyweight) {
      accepted-inputs (bodyweight, bodyweightUnit)
      local-endpoint ("RegisterBodyweight.js")
    }
    action-endpoint (RegisterHangover) {
      local-endpoint ("RegisterHangover.js")
    }
    action-endpoint (RequestBAC) {
      local-endpoint ("RequestBAC.js")
    }
  }
}
